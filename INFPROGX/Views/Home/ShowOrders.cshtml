@model IEnumerable<INFPROGX.Models.Order>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
    <h1>@ViewBag.Message</h1>
        @foreach(var item in Model) {
           <span>Order Id: </span>@item.OrderId;<br />
           <span>Order date: </span>@item.Date;<br />
           @Html.ActionLink("Show Detail", "Detail", new { id = item.OrderId }) @Html.Raw("|")}            
        }

    </div>
</body>
</html>
